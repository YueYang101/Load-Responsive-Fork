    Index=-1,
    Variables=["Young's Modulus"],
    Values=[["69 [MPa]"]])
matlProp1.SetVariableProperty(
    VariableName="Young's Modulus",
    Property="Unit",
    Value="GPa")
matlProp1.SetData(
    Variables=["Young's Modulus"],
    Values=[["0.069 [GPa]"]])
matlProp1.SetData(
    Variables=["Young's Modulus"],
    Values=[["190 [GPa]"]])
matlProp1.SetData(
    Variables=["Poisson's Ratio"],
    Values=[["0.33"]])
